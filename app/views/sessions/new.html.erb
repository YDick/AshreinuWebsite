<div class = "mailer" >

<h1>Login</h1>

<%= form_for :session, url: session_path do |f| %>
 <div>
  <%= f.label :username %>
  <%= f.text_field :username %>
 </div>

 <div>
  <%= f.label :password %>
  <%= f.password_field :password %>
 </div>
  <%= f.submit "login" %>
<%end%>

<%= link_to "Make an Account!", new_fundraiser_path %>

</div>